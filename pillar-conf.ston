{
    "title" : "Enterprise Pharo",
    "newLine" : #unix,
    "configurations" : {
        "LaTeX by chapter" : {
            "outputType" : #latex,
            "template" : "chapter.latex.template",
            "headingLevelOffset" : 1,
            "separateOutputFiles" : true
        },
        "LaTeX whole book" : {
            "outputType" : #latex,
            "headingLevelOffset" : 1,
            "template" : "book.latex.template",
            "outputFile" : "EnterprisePharo.tex"
        },
        "HTML by chapter" : {
            "outputType" : #html,
            "headingLevelOffset" : 1,
